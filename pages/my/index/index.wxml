<view class="usercenter" >
  <view class="header" >
    <view  class="userinfo">
      <image class="userinfo-avatar" src="{{userInfo.avatarUrl}}"  background-size="cover"></image>
      <view>
        <view class="userinfo-nickname">{{userInfo.nickName}}</view>
        <!--<view class="userinfo-lv">1级</view>-->
      </view>
    </view>
  </view>

  
  <view class="menu-container">
    <view class="menu-item" bindtap="gotoMyTaste" >
      <view class="left" >
        <image src="../../../assets/images/taste.png" class="menu-item-icon" background-size="cover"></image>
        <text>我的体验</text>
      </view>
      <view class="right"> <image src="../../../assets/images/right.png"  background-size="cover"></image></view>
    </view>

    <view class="menu-item" bindtap="gotoMyShare">
      <view class="left" >
        <image src="../../../assets/images/me-share.png" class="menu-item-icon" background-size="cover"></image>
        <text>我的分享</text>
      </view>
      <view class="right"> <image src="../../../assets/images/right.png"  background-size="cover"></image></view>
    </view>
  </view>


  <view class="menu-container">
    <view class="menu-item" bindtap="gotoTasteLincense">
      <view class="left" >
        <image style="width: 40rpx;height: 40rpx;"   src="../../../assets/images/license.png" class="menu-item-icon" background-size="cover"></image>
        <text>体验执照</text>
      </view>
      <view class="right"> <image src="../../../assets/images/right.png"  background-size="cover"></image></view>
    </view>
  </view>
  
</view>